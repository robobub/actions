{
  "name": "robobub-actions.worker",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy",
    "lint": "eslint .",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
