{
  "name": "@robobub/worker",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy",
    "lint": "eslint .",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings --env-file ./.dev.vars.example",
    "typecheck": "pnpm run cf-typegen && tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@octokit/core": "7.0.6",
    "@octokit/plugin-paginate-rest": "14.0.0",
    "hono": "4.11.4",
    "mri": "1.2.0",
    "semver": "7.7.3"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.10.7",
    "@cloudflare/workers-types": "4.20251115.0",
    "@luxass/eslint-config": "6.0.3",
    "@types/semver": "7.7.1",
    "eslint": "9.39.1",
    "eslint-plugin-format": "1.0.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "wrangler": "4.47.0"
  }
}
