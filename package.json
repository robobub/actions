    "test:watch": "vitest"
  },
  "dependencies": {
    "@octokit/core": "^6.1.6",
    "@octokit/plugin-paginate-rest": "^13.1.1",
    "hono": "^4.8.5",
    "mri": "^1.2.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.55",
    "@cloudflare/workers-types": "^4.20250719.0",
    "@luxass/eslint-config": "^5.1.1",
    "@types/semver": "^7.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.0"
  }
